<div class="jumbotron">
  <p><h1><%= t('title_home_index') %></h1></p>
    <%= t('body_home_index_html') %>
  <p>
    <%= t('body_feedback_home_index_html', href: link_to('feedback', 'https://github.com/mezuro/mezuro/issues', target: '_blank')) %>
  </p>
</div>

<h2><%= t('latest_projects') %></h2>

<ul>
  <% cache do %>
